[environment:default]
OVEHOSTED_CORE/confirmSettings=bool:True
OVEHOSTED_CORE/deployProceed=bool:True
OVEHOSTED_CORE/screenProceed=bool:True
OVEHOSTED_CORE/skipTTYCheck=bool:True
OVEHOSTED_ENGINE/adminPassword=str:{{ rhvm_password_admin }}
OVEHOSTED_ENGINE/clusterName=str:{{ cluster_name }}
OVEHOSTED_ENGINE/datacenterName=str:{{ datacenter_name }}
OVEHOSTED_ENGINE/enableHcGlusterService=none:None
OVEHOSTED_ENGINE/insecureSSL=none:None
OVEHOSTED_NETWORK/bridgeIf=str:{{ networks.nics[0].device if not networks.bonds or networks.bonds|length < 2 else networks.bonds[1].device }}
OVEHOSTED_NETWORK/bridgeName=str:ovirtmgmt
OVEHOSTED_NETWORK/fqdn=str:{{ hostvars[groups.rhvm[0]].hostname }}
OVEHOSTED_NETWORK/gateway=str:{{ gateway }}
OVEHOSTED_NETWORK/host_name=str:{{ hostname }}
OVEHOSTED_NETWORK/network_test=str:ping
OVEHOSTED_NETWORK/network_test_tcp_address=none:None
OVEHOSTED_NETWORK/network_test_tcp_port=none:None
OVEHOSTED_NOTIF/destEmail=str:root@localhost
OVEHOSTED_NOTIF/smtpPort=str:25
OVEHOSTED_NOTIF/smtpServer=str:localhost
OVEHOSTED_NOTIF/sourceEmail=str:root@localhost
OVEHOSTED_STORAGE/LunID=str:{{ fc_lun_id_engine }}
OVEHOSTED_STORAGE/discardSupport=bool:True
OVEHOSTED_STORAGE/domainType=str:{{ storage_type }}
OVEHOSTED_STORAGE/iSCSIDiscoverUser=none:None
OVEHOSTED_STORAGE/iSCSIPortal=none:None
OVEHOSTED_STORAGE/iSCSIPortalIPAddress=none:None
OVEHOSTED_STORAGE/iSCSIPortalPort=none:None
OVEHOSTED_STORAGE/iSCSIPortalUser=none:None
OVEHOSTED_STORAGE/iSCSITargetName=none:None
OVEHOSTED_STORAGE/imgSizeGB=str:{{ rhvm_vm_image_size }}
OVEHOSTED_STORAGE/imgUUID=str:{{ 99999999 | random | to_uuid }}
OVEHOSTED_STORAGE/lockspaceImageUUID=none:None
OVEHOSTED_STORAGE/lockspaceVolumeUUID=none:None
OVEHOSTED_STORAGE/metadataImageUUID=none:None
OVEHOSTED_STORAGE/metadataVolumeUUID=none:None
OVEHOSTED_STORAGE/mntOptions=str:
OVEHOSTED_STORAGE/nfsVersion=str:auto
OVEHOSTED_STORAGE/storageDomainConnection=str:{{ nfs_server }}:{{ nfs_path_engine }}
OVEHOSTED_STORAGE/storageDomainName=str:hosted_storage
OVEHOSTED_STORAGE/volUUID=str:{{ 99999999 | random | to_uuid }}
OVEHOSTED_VM/applyOpenScapProfile=bool:False
OVEHOSTED_VM/automateVMShutdown=bool:True
OVEHOSTED_VM/cdromUUID=str:{{ 99999999 | random | to_uuid }}
OVEHOSTED_VM/cloudinitISO=str:generate
OVEHOSTED_VM/cloudinitExecuteEngineSetup=bool:True
OVEHOSTED_VM/cloudinitInstanceDomainName=str:{{ cluster_domain }}
OVEHOSTED_VM/cloudinitInstanceHostName=str:{{ hostvars[groups.rhvm[0]].hostname }}
OVEHOSTED_VM/cloudinitRootPwd=str:{{ rhvm_password_root }}
OVEHOSTED_VM/cloudinitVMDNS=str:{{ name_servers | join(',') }}
OVEHOSTED_VM/cloudinitVMETCHOSTS=bool:False
OVEHOSTED_VM/cloudinitVMStaticCIDR=str:{{ hostvars[groups.rhvm[0]].ip }}/{{ cluster_ip_prefix }}
OVEHOSTED_VM/cloudinitVMTZ=str:{{ timezone }}
OVEHOSTED_VM/consoleUUID=str:{{ 99999999 | random | to_uuid }}
OVEHOSTED_VM/emulatedMachine=str:pc-i440fx-rhel7.3.0
OVEHOSTED_VM/nicUUID=str:{{ 99999999 | random | to_uuid }}
OVEHOSTED_VM/ovfArchive=str:
OVEHOSTED_VM/rootSshAccess=str:yes
OVEHOSTED_VM/rootSshPubkey=str:{{ lookup('file', '~/.ssh/id_rsa.pub') }}
OVEHOSTED_VM/vmCDRom=none:None
OVEHOSTED_VM/vmMACAddr=str:{{ "00:16:3e" | random_mac }}
OVEHOSTED_VM/vmMemSizeMB=int:{{ rhvm_vm_memory }}
OVEHOSTED_VM/vmVCpus=str:{{ rhvm_vm_vcpus }}
